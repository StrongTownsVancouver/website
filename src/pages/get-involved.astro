---
import BaseHead from '../components/BaseHead.astro'
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts'
import BrevoSubscribeStyles from '../components/BrevoSubscribeStyles.astro'

const PAGE_TITLE = `Get Involved - ${SITE_TITLE}`
---

<html lang="en">
    <head>
        <BaseHead
            title={PAGE_TITLE}
            description={SITE_DESCRIPTION}
            image="logo/stvanlogo_l-blue_d-blue.png"
        />
        <BrevoSubscribeStyles />
        <script>
            document.location.href = '/'
        </script>
    </head>
</html>
