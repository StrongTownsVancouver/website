---
import ContentWithBackground from './ContentWithBackground.astro'

interface Props {
    title: string
    image: string
}

const { title, image } = Astro.props
---

<ContentWithBackground background={image}>
    <div class="flex justify-center p-12">
        <h3 class="font-synthese m-0 text-3xl mb-2 text-white">{title}</h3>
    </div>
</ContentWithBackground>
